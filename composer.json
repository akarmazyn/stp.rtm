    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "~2.3",
        "zircote/swagger-php": "0.8.1",
        "doctrine/doctrine-mongo-odm-module": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/phpcov": "~2.0",
